List.map((item) => {\n      return (\n        <SubMenu title={item.authName} key={item.id}>\n          {item.children.map((item) => {\n            return (\n              <Menu.Item\n                title={item.authName}\n                key={item.id}\n                style={{ color: '#fff' }}\n              >\n                <Link to={item.path}>{item.authName}</Link>\n                {item.path}\n              </Menu.Item>\n            )\n          })}\n        </SubMenu>\n      )\n    })\n  }\n  render() {\n    return (\n      <div>\n        <Layout className=\"home-container\">\n          <div className=\"home-header\">\n            <Header className=\"title\">商城后台管理系统</Header>\n            <Button className=\"btn\" onClick={this.loginout}>\n              退出\n            </Button>\n          </div>\n          <Layout>\n            <Sider width={200} style={{ background: '#fff' }}>\n              <Menu\n                mode=\"inline\"\n                defaultSelectedKeys={['1']}\n                defaultOpenKeys={['sub1']}\n                style={{ height: '100%', borderRight: 0 }}\n              >\n                {this.mountedMenuList()}\n              </Menu>\n            </Sider>\n            <Layout style={{ padding: '0 24px 24px' }}>\n              <Content></Content>\n            </Layout>\n          </Layout>\n        </Layout>\n      </div>\n    )\n  }\n}\n",{"ruleId":"16","severity":1,"message":"17","line":3,"column":24,"nodeType":"18","messageId":"19","endLine":3,"endColumn":28},"no-unused-vars","'Icon' is defined but never used.","Identifier","unusedVar"]